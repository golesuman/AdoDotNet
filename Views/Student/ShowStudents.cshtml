@model List<Student>


<h1>All Students</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Roll</th>
            <th scope="col">Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Roll</td>
                <td>@student.Address</td>
            </tr>
        }
    </tbody>
</table>
